<txnmgr-hq class="org.jpos.transaction.TransactionManager" logger="Q2">
 <property name="queue"            value="HQTXNMGR" />
 <property name="sessions"         value="10" />
 <property name="persistent-space" value="jdbm:hqtxnmgr" />
 <property name="debug"            value="true" />
 <property name="retry-interval"   value="250" />
 <property name="pause-timeout"   value="2000" />
  
 <participant class="com.hqsolution.hqserver.app.core.Switch" logger="Q2">
  
   <!-- <property name="027207" value="Redeem"/>
        <property name="027567" value="BalanceInquiry"/>
